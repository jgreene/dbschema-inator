{
    "name": "dbschema-inator",
    "version": "0.0.04",
    "description": "fetch sql database schema into a friendly data model for code generation",
    "license": "MIT",
    "files": [
      "lib"
    ],
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "scripts": {
      "clean": "rimraf dist",
      "test": "mocha -r ts-node/register src/**/*.spec.ts",
      "testintegration": "mocha -r ts-node/register src/**/*.integration.ts --timeout 15000",
      "watch": "npm run test -- --watch-extensions ts --watch -R min",
      "build": "tsc -d -p ."
    },
    "dependencies": {
      "@types/mssql": "^4.0.10",
      "@types/mocha": "^5.2.5",
      "@types/chai": "^4.1.6",
      "mssql": "^4.2.1",
      "msnodesqlv8": "^0.6.8",
      "mocha": "^5.2.0",
      "chai": "^4.2.0"
    },
    "devDependencies": {
      "rimraf": "^2.6.2",
      "typescript": "^3.1.1",
      "ts-node": "^7.0.0"
    }
  }