      "build": "tsc -b"
    },
    "devDependencies": {
      "@types/mocha": "^5.2.5",
      "@types/chai": "^4.1.6",
      "mocha": "^5.2.0",
      "chai": "^4.2.0",
      "rimraf": "^2.6.2",
      "typescript": "^3.2.4",
      "ts-node": "^7.0.1"
    }
  }