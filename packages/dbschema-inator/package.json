{
    "name": "dbschema-inator",
    "version": "0.0.7",
    "description": "fetch sql database schema into a friendly data model for code generation",
    "license": "MIT",
    "files": [
      "lib"
    ],
    "main": "lib/index.js",
    "typings": "lib/index.d.ts",
    "scripts": {
      "clean": "rimraf lib",
      "test": "mocha -r ts-node/register src/**/*.spec.ts",
